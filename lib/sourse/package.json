  "scripts": {
    "mock": "json-server -w -p 8981 -q false ./database/db.json",
    "start": "node ./config/server.js",
    "analyse": "webpack --progress --json > stats.json --config ./config/webpack.config.prod.js && rimraf app",
    "join": "set DEV_MODULE=join && node ./config/server.js",
    "release": "rimraf dev && webpack --progress --profile --color --config ./config/webpack.config.prod.js ",
    "build": "rimraf dist && rimraf *.zip &&  set BUILD_MODULE=build && webpack --progress --profile --color --config ./config/webpack.config.prod.js && cp -r dist midbak && node config/bak.js",
    "postrelease": "fjpublish env test -y --diff --config ./publish.config.js&& cp -r dev midbak && node config/bak.js"
  },
